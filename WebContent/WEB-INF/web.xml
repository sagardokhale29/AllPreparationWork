<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
 <display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>jmsPublish</servlet-name>
		<servlet-class>com.pamu.jms.servlet.IBMMQPublishServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>jmsSubscribe</servlet-name>
		<servlet-class>com.pamu.jms.servlet.IBMMQSubscriberServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>jmsPublish</servlet-name>
		<url-pattern>/publish</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>jmsSubscribe</servlet-name>
		<url-pattern>/subscribe</url-pattern>
	</servlet-mapping>
</web-app>
